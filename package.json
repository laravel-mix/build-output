{
  "name": "@laravel-mix/webpack-build-output",
  "version": "0.0.1",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "lint:check": "prettier -c ./src ./__tests__",
    "lint:fix": "prettier -w ./src ./__tests__",
    "lint:staged": "pretty-quick --staged",
    "test": "jest --runInBand",
    "build": "rollup -c"
  },
  "dependencies": {
    "picocolors": "^0.2.1",
    "strip-ansi": "^6.0.1",
    "table": "^6.7.2"
  },
  "peerDependencies": {
    "webpack": "^5.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "rollup": "^2.58.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-prettier": "^2.2.0",
    "simple-git-hooks": "^2.6.1",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "webpack": "^5.58.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint:staged"
  }
}
